<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/onBoardingFragment">
    <fragment
        android:id="@+id/onBoardingFragment"
        android:name="how.about.it.view.onboarding.OnBoardingFragment"
        android:label="fragment_on_boarding"
        tools:layout="@layout/fragment_on_boarding">
        <action
            android:id="@+id/action_onBoardingFragment_to_feedFragment"
            app:destination="@id/feedFragment"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/feedFragment"
        android:name="how.about.it.view.feed.FeedFragment"
        android:label="fragment_feed"
        tools:layout="@layout/fragment_feed">
        <action
            android:id="@+id/action_feedFragment_to_writeFragment"
            app:destination="@id/writeFragment" />
        <action
            android:id="@+id/action_feedFragment_to_notificationFragment"
            app:destination="@id/notificationFragment" />
    </fragment>
    <fragment
        android:id="@+id/notificationFragment"
        android:name="how.about.it.view.notification.NotificationFragment"
        android:label="fragment_notification"
        tools:layout="@layout/fragment_notification" />
    <fragment
        android:id="@+id/writeFragment"
        android:name="how.about.it.view.write.WriteFragment"
        android:label="fragment_write"
        tools:layout="@layout/fragment_write">
        <action
            android:id="@+id/action_writeFragment_to_tempListWriteFragment"
            app:destination="@id/tempListWriteFragment" />
    </fragment>
    <fragment
        android:id="@+id/tempListWriteFragment"
        android:name="how.about.it.view.write.TempListWriteFragment"
        android:label="fragment_temp_list_write"
        tools:layout="@layout/fragment_temp_list_write">
        <action
            android:id="@+id/action_tempListWriteFragment_to_tempSavedWriteFragment"
            app:destination="@id/tempSavedWriteFragment">
            <argument
                android:name="tempPostTitle"
                app:argType="string"
                android:defaultValue="" />
            <argument
                android:name="tempPostContent"
                app:argType="string"
                android:defaultValue="" />
            <argument
                android:name="tempPostImage"
                app:argType="string"
                android:defaultValue="" />
        </action>
    </fragment>
    <fragment
        android:id="@+id/tempSavedWriteFragment"
        android:name="how.about.it.view.write.TempSavedWriteFragment"
        android:label="fragment_temp_saved_write"
        tools:layout="@layout/fragment_temp_saved_write">
        <action
            android:id="@+id/action_tempSavedWriteFragment_to_writeFragment"
            app:destination="@id/writeFragment"
            app:popUpTo="@id/writeFragment"
            app:popUpToInclusive="true">
            <argument
                android:name="tempPostTitle"
                app:argType="string"
                android:defaultValue="" />
            <argument
                android:name="tempPostContent"
                app:argType="string"
                android:defaultValue="" />
            <argument
                android:name="tempPostImage"
                app:argType="string"
                android:defaultValue="" />
        </action>
    </fragment>
    <fragment
        android:id="@+id/myPageFragment"
        android:name="how.about.it.view.mypage.MyPageFragment"
        android:label="fragment_my_page"
        tools:layout="@layout/fragment_my_page">
        <action
            android:id="@+id/action_myPageFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_myPageFragment_to_settingFragment"
            app:destination="@id/settingFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="how.about.it.view.profile.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/settingFragment"
        android:name="how.about.it.view.setting.SettingFragment"
        android:label="fragment_setting"
        tools:layout="@layout/fragment_setting" />
    <fragment
        android:id="@+id/commentFragment"
        android:name="how.about.it.view.comment.CommentFragment"
        android:label="fragment_comment"
        tools:layout="@layout/fragment_comment" />
    <fragment
        android:id="@+id/detailPostFragment"
        android:name="how.about.it.view.detailPost.DetailPostFragment"
        android:label="fragment_detail_post"
        tools:layout="@layout/fragment_detail_post" />
</navigation>